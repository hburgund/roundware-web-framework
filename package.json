{
  "name": "roundware-web-framework",
  "version": "0.0.1-alpha.0",
  "description": "Client framework for Roundware, a location-based contributory audio platform",
  "main": "roundware.js",
  "files": "src",
  "scripts": {
    "test": "./node_modules/.bin/babel-node ./node_modules/.bin/jasmine",
    "cover": "nyc --reporter=lcov npm t && open ./coverage/lcov-report/index.html",
    "check-coverage": "nyc check-coverage --statement 100 --branches 100 --function 100 --lines 100",
    "prebuild": "rimraf dist",
    "build": "npm-run-all --parallel build:*",
    "build:main": "babel --copy-files --out-dir dist src",
    "build:umd": "webpack --output-filename roundware-web.umd.js",
    "build:umd.min": "webpack --output-filename roundware-web.umd.min.js -p",
    "docbuild": "jsdoc --verbose -c ./config/jsdoc.json",
    "devstart": "webpack-dev-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/roundware/roundware-web.git"
  },
  "keywords": [
    "audio",
    "roundware"
  ],
  "author": "Mike Subelsky <mike@subelsky.com> (http://www.subelsky.com/)",
  "license": "Affero GPL",
  "bugs": {
    "url": "https://github.com/roundware/roundware-web/issues"
  },
  "homepage": "https://roundware.github.io/roundware-web-framework/",
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-loader": "7.0.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "file-loader": "0.11.1",
    "jasmine": "2.6.0",
    "jsdoc": "3.4.3",
    "json-loader": "0.5.4",
    "npm-run-all": "4.0.2",
    "nyc": "10.3.2",
    "path": "0.12.7",
    "pryjs": "1.0.3",
    "rimraf": "2.6.1",
    "webpack": "2.5.1",
    "webpack-dev-server": "2.4.5"
  },
  "jshintConfig": {
    "esversion": 6
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "dependencies": {
    "jquery": "3.2.1"
  }
}
